<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;com.training.facades.product.data.GenderData>"/>
		<property name="questions" type="java.util.List&lt;com.training.questions.data.QuestionData&gt;" />
		<property name="questionsCount" type="java.lang.Integer" />
	</bean>

	<bean class="com.training.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>

	<bean class="com.training.questions.data.QuestionData">
		<property name="code" type="String" />
		<property name="question" type="String" />
		<property name="questionCustomer" type="String" />
		<property name="answer" type="String" />
		<property name="answerCustomer" type="String" />
	</bean>
</beans>
